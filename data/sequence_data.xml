<odoo>
    <record id="clinic_appointment_sequence" model="ir.sequence">
        <field name="name">Appointment Sequence</field>
        <field name="code">clinic.appointment.sequence</field>
        <field name="prefix">CA</field> <!--CA: Clinic Appointment -->
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>
    
    <record id="clinic_patient_sequence" model="ir.sequence">
        <field name="name">Clinic Patient</field>
        <field name="code">clinic.patient</field>
         <field name="prefix">CP</field> <!--CP:clinc patient -->
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="clinic_treatment_sequence" model="ir.sequence">
        <field name="name">Clinic Treatment</field>
        <field name="code">clinic.treatment</field>
         <field name="prefix">CT</field> <!--CT:clinc Treatment -->
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="clinic_prescription_sequence" model="ir.sequence">
        <field name="name">Clinic Prescription</field>
        <field name="code">clinic.prescription</field>
         <field name="prefix">PRSC</field> <!--PRSC:clinc Prescription -->
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>